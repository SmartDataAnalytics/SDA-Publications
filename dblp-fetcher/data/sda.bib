  doi          = {10.1145/2993318.2993320},
  keywords     = {168-1466, 2016, 71-4882, baron, esteves, group\_aksw, hobbit, lehmann, mex, mole, moussallem, projecthobbit, sda, sda-pub, sys:relevantFor:bis, sys:relevantFor:infai},
  pages        = {17--24},
  publisher    = {ACM},
  pubstate     = {published},
