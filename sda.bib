  keywords      = {75-8883 20-7514 155-0002 185-0765 141-0902 93-7184 143-9337 71-4882 sda-pub 2017 bde sansa dsa lehmann sejdiu buehmann westphal stadler bin chakraborty group_aksw iermilov ngonga saleem jabeen},
  pages         = {147--155},
  publisher     = {Springer},
  series        = {Lecture Notes in Computer Science},
  timestamp     = {Tue, 14 May 2019 10:00:53 +0200},
