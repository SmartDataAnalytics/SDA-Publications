  pubstate     = {published},
  school       = {University of Bonn, Germany},
  timestamp    = {Wed, 04 May 2022 01:00:00 +0200},
  title        = {{S}trategies for a {S}emantified {U}niform {A}ccess to {L}arge and {H}eterogeneous
{D}ata {S}ources},
  tppubtype    = {phdthesis},
  url          = {https://hdl.handle.net/20.500.11811/8925},
