  interhash             = {0a8d64c53e0a6910aa7a9b612066bb95},
  intrahash             = {7b921d585db77803ee9c0d9f4c0c1c77},
  isbn                  = {978-3-642-41153-3},
  keywords              = {2013 group_aksw event_wise khalili auer rdface sys:relevantFor:infai sys:relevantFor:bis sys:relevantFor:lod2 peer-reviewed},
  pages                 = {425-438},
  publisher             = {Springer Berlin Heidelberg},
  series                = {Lecture Notes in Computer Science},
  timestamp             = {2013-07-18T11:27:45.000+0200},
  title                 = {W{Y}SI{W}YM authoring of structured content based on Schema.org},
  url                   = {http://dx.doi.org/10.1007/978-3-642-41154-0_32},
  volume                = {8181},
  year                  = {2013}
