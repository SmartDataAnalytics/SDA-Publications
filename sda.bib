  doi           = {10.1108/00330331211221828},
  journal       = {Program},
  keywords      = {71-4882 20-7514 2012 group_aksw MOLE sys:relevantFor:bis sys:relevantFor:lod2 morsey lehmann auer stadler hellmann kilt},
  number        = {2},
  pages         = {157--181},
  timestamp     = {Wed, 30 Jan 2019 00:00:00 +0100},
  title         = {D{B}pedia and the live extraction of structured data from Wikipedia},
  url           = {http://svn.aksw.org/papers/2011/DBpedia_Live/public.pdf},
  volume        = {46},
  year          = {2012}
}

