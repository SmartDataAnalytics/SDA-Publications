  note         = {17.3\% acceptance rate, Best paper award},
  pubstate     = {published},
  title        = {{T}he {R}DFa {C}ontent {E}ditor - {F}rom {W}YS{I}WY{G} to {W}YS{I}WY{M}},
  tppubtype    = {inproceedings},
  url          = {http://svn.aksw.org/papers/2012/COMPSAC\_RDFaCE/public.pdf},
  year         = {2012}
}

