  bibsource     = {dblp computer science bibliography, https://dblp.org},
  biburl        = {https://dblp.org/rec/conf/semweb/LehmannSBWSEBCS17.bib},
  booktitle     = {The Semantic Web - ISWC 2017 - 16th International Semantic Web Conference,
