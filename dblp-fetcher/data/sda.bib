  title        = {{T}owards an {O}pen-{G}overnmental {D}ata {W}eb},
  tppubtype    = {inproceedings},
  url          = {http://svn.aksw.org/papers/2012/KESW2012\_OpenGovWeb/public.pdf},
  year         = {2012}
}

